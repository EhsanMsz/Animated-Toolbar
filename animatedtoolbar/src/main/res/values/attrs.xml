<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimatedToolbar">
        <attr name="animatedToolbarStyle" format="reference" />
        <attr name="duration" format="integer" />
        <attr name="angle" format="float" />
        <attr name="shape">
            <enum name="ramp" value="0" />
            <enum name="rounded" value="1" />
            <enum name="curved" value="2" />
        </attr>
        <attr name="shapeGravity">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="animationEnabled" format="boolean" />
    </declare-styleable>
</resources>